  import cv2
cap = cv2.VideoCapture('input_video.mp4')
fourcc = cv2.VideoWriter_fourcc(*'mp4v')
out = cv2.VideoWriter('output_video.mp4', fourcc, 30, (843, 480))
while cap.isOpened():
    ret, frame = cap.read()
    if not ret:
        break
    if cap.get(cv2.CAP_PROP_POS_FRAMES) % 4 == 0:
        out.write(frame)
cap.release()
out.release()
